#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/e/vuepress/my-project/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_cd3d3f152fae5bd1ff5e9c0242f9619a/node_modules/vuepress/bin/node_modules:/mnt/e/vuepress/my-project/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_cd3d3f152fae5bd1ff5e9c0242f9619a/node_modules/vuepress/node_modules:/mnt/e/vuepress/my-project/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_cd3d3f152fae5bd1ff5e9c0242f9619a/node_modules:/mnt/e/vuepress/my-project/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/e/vuepress/my-project/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_cd3d3f152fae5bd1ff5e9c0242f9619a/node_modules/vuepress/bin/node_modules:/mnt/e/vuepress/my-project/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_cd3d3f152fae5bd1ff5e9c0242f9619a/node_modules/vuepress/node_modules:/mnt/e/vuepress/my-project/node_modules/.pnpm/vuepress@2.0.0-rc.20_@vuepr_cd3d3f152fae5bd1ff5e9c0242f9619a/node_modules:/mnt/e/vuepress/my-project/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../bin/vuepress.js" "$@"
else
  exec node  "$basedir/../../bin/vuepress.js" "$@"
fi
